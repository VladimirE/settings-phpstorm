  </template>
  <template name="getc" value="get-color('additional', 'darker')" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="empty" value="{% if block.children is not empty %}&#10;{% endif %}" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="remove" value="- '@remove':&#10;    id: featured_products_container" description="" toReformat="false" toShortenFQNames="true">
